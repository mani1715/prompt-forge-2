# =============================================================================
# DEVELOPMENT FRONTEND ENVIRONMENT VARIABLES
# =============================================================================

# Backend API URL (uses proxy in development)
REACT_APP_BACKEND_URL=/api
REACT_APP_API_VERSION=v1

# Application
REACT_APP_NAME="MSPN DEV"
REACT_APP_VERSION="1.0.0-dev"
REACT_APP_ENVIRONMENT=development

# Features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_CHAT=true

# Webpack Dev Server
WDS_SOCKET_PORT=443
USE_WEBPACK_PROXY=true
BACKEND_PROTOCOL=http:
BACKEND_HOST=localhost:8001
ENABLE_HEALTH_CHECK=false

# Build
GENERATE_SOURCEMAP=true
REACT_APP_ENABLE_PROFILING=true

# Hot Reload
DISABLE_HOT_RELOAD=false
FAST_REFRESH=true