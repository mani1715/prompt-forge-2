# =============================================================================
# PRODUCTION FRONTEND ENVIRONMENT VARIABLES
# =============================================================================

# Backend API URL
REACT_APP_BACKEND_URL=https://your-api.onrender.com/api
REACT_APP_API_VERSION=v1

# Application
REACT_APP_NAME="MSPN DEV"
REACT_APP_VERSION="1.0.0"
REACT_APP_ENVIRONMENT=production

# Features
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT=true

# Monitoring
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Analytics
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Social
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_TWITTER_HANDLE=@yourtwitterhandle

# Build Optimization
GENERATE_SOURCEMAP=false
REACT_APP_ENABLE_PROFILING=false

# Webpack Dev Server (not used in production)
WDS_SOCKET_PORT=443
USE_WEBPACK_PROXY=false
ENABLE_HEALTH_CHECK=false